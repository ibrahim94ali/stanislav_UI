{
  "expo": {
    "name": "Stanislove",
    "slug": "stanislove",
    "description": "Stanisove is an app to find you the best houses, apartments, shops, lands, villas for rent and for purchase in North Macedonia. There are tens of useful filters to help you find what you are looking for.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/s_icon_bg.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F1EB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyDGXVlzF0pQCDuO0Ni3amsZhqxNKAX9Ymc"
      }
    },
    "android": {
      "package": "com.ibrahimaliu.stanislove",
      "versionCode": 3,
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/s_icon_bg.png",
        "backgroundColor": "#F6F1EB"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB_UpFIRpQoPllkBWTEOYAqbAXJb_8kQPk"
        }
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
