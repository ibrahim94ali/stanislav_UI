{
  "expo": {
    "name": "Stanislove",
    "slug": "stanislove",
    "description": "Stanisove is an app to find you the best houses, apartments, shops, lands, villas for rent and for purchase in North Macedonia. There are tens of useful filters to help you find what you are looking for.",
    "version": "1.1.3",
    "orientation": "portrait",
    "icon": "./assets/images/s_icon_bg.png",
    "scheme": "stanislove",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F1EB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ibrahimaliu.stanislove",
      "config": {
        "googleMapsApiKey": "AIzaSyDGXVlzF0pQCDuO0Ni3amsZhqxNKAX9Ymc"
      }
    },
    "android": {
      "package": "com.ibrahimaliu.stanislove",
      "versionCode": 10,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE",
        "READ_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/s_adaptive_icon.png",
        "backgroundColor": "#F6F1EB"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB_UpFIRpQoPllkBWTEOYAqbAXJb_8kQPk"
        }
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share your property photos when adding a new property."
        }
      ]
    ]
  }
}
